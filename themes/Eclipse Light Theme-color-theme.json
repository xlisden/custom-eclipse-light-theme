				"fontStyle": ""
			}
		},
		{
			"scope": "markup.quote",
			"settings": {
				"foreground": "#643A3A",
				"fontStyle": ""
			}
		},
		{
			"scope": "markup.fenced_code",
			"settings": {
				"foreground": "#0701B4",
				"fontStyle": ""
			}
		},
		{
			"scope": "markup.inline.raw",
			"settings": {
				"foreground": "#2D0BF2",
				"fontStyle": ""
			}
		},
		{
			"scope": "comment",
			"settings": {
			  "foreground": "#b9b9b9",
			  "fontStyle": "italic"
			}
		}
		  
	]
