				"fontStyle": ""
			}
		},
		{
			"scope": "entity.name.type",
			"settings": {
				"foreground": "#000000",
				"fontStyle": ""
			}
		},
		{
			"scope": "constant",
			"settings": {
				"foreground": "#000000",
				"fontStyle": "bold italic"
			}
		},
		{
			"scope": "support.type.property-name",
			"settings": {
				"foreground": "#0701B4",
				"fontStyle": ""
			}
		},
		{
			"scope": "entity.name.type.lifetime",
			"settings": {
