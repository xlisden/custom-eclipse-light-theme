		"parameter": {
			"foreground": "#643A3A",
			"fontStyle": ""
		},
		"string": {
			"foreground": "#2D0BF2",
			"fontStyle": ""
		},
		"characterLiteral": {
			"foreground": "#2D0BF2",
			"fontStyle": ""
